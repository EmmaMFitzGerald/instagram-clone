<link href="../css/header.style.css" rel="stylesheet" type="text/css">
<link href="https://fonts.googleapis.com/css2?family=Concert+One&display=swap" rel="stylesheet">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<body>
  <header>
    <a href="/explore">
      <img src="../images/Image.jpg" alt="PhotoBook Logo" class="logo"/> 
      </a>
        <nav>
            <ul class="navlinks">
                <% if (userName) { %>
                <li><a href="/profile/<%=userName%>"><%= userName %></a>'s Profile</li>
                <li><form method="post" action="/search">
                  <input type="text" placeholder="Search for Users" name="search">
                  <button type="submit"><i class="fa fa-search"></i></button>
                </form></li>
                <li><a href="/explore">Explore</a></li>
                <li class="signout"><a href="/signout">Sign Out</a></li>
                  <% } else { %>
                <li><a href="/signup">Sign Up</a></li>
                <li><a href="/signin">Sign In<a></li>
                  <% } %>
            </ul>
          </nav>
</header>
</body>